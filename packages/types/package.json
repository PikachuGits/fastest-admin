{
  "name": "@fastest/types",
  "version": "0.1.0",
  "description": "Shared TypeScript type definitions for the fastest monorepo",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "@mui/material": "catalog:",
    "@mui/lab": "catalog:"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "@types/react": "catalog:"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./theme": {
      "types": "./src/theme/index.ts",
      "default": "./src/theme/index.ts"
    },
    "./layout": {
      "types": "./src/layout/index.ts",
      "default": "./src/layout/index.ts"
    }
  }
}